<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings file="..\..\App.private.config">

    <!-- Replace with your own private key, Base64 encoded -->
    <add key="KP.GmailClient.PrivateKey" value="" />
    <add key="KP.GmailClient.TokenUri" value="https://accounts.google.com/o/oauth2/token" />
    <add key="KP.GmailClient.ClientEmail" value="your-project-name@your-project-name.iam.gserviceaccount.com" />
    <add key="KP.GmailClient.EmailAddress" value="youremail@gmail.com" />
  </appSettings>
</configuration>
